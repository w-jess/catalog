<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="seq_catalog_sale" model="ir.sequence">
            <field name="name">Sale Sequence</field>
            <field name="code">catalog.sale</field>
            <field name="prefix">S</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_catalog_purchase" model="ir.sequence">
            <field name="name">Purchase Sequence</field>
            <field name="code">catalog.purchase</field>
            <field name="prefix">P</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_catalog_product" model="ir.sequence">
            <field name="name">Product Sequence</field>
            <field name="code">catalog.product</field>
            <field name="prefix">Pr</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_catalog_deal" model="ir.sequence">
            <field name="name">库存条码号</field>
            <field name="code">catalog.deal</field>
            <field name="prefix">SKD%(year)s%(month)s%(day)s</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_catalog_picking" model="ir.sequence">
            <field name="name">出库单号</field>
            <field name="code">catalog.picking</field>
            <field name="prefix">PI%(year)s%(month)s%(day)s</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>